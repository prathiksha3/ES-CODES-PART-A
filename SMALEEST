AREA SMALLEST,CODE,READONLY
ENTRY
START
  MOV R5,#6
  LDR R1,=VALUE1
  LDR R2,[R1],#4
LOOP
  LDR R4,[R1],#4
  CMP R2,R4
  BLE LOOP0
  MOV R2,R4
LOOP0
  SUBS R5,R5,#1
  CMP R5,#0
  LDR R4,= RESULT
  STR R4,[R2]
B1 B B1
VALUE1
  DCD 0X88888888
  DCD 0X22222222
  DCD 0X44444444
  DCD 0X33333333
  DCD 0XAAAAAAAA
  DCD 0X11111111
  DCD 0X55555555
  DCD 0X33333333
 AREA DATA1,DATA,READWRITE
RESULT DCD 0X00
	  END
